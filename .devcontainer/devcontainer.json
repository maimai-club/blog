{
  "name": "vitepress-blog",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "remoteUser": "node",
  "portsAttributes": {
    "5173": {
      "label": "Application"
    }
  },
  "forwardPorts": [
    5173
  ],
  "customizations": {
    "vscode": {
      "extensions": [
        "Vue.volar",
        "Vue.vscode-typescript-vue-plugin",
        "mosapride.zenkaku",
        "oderwat.indent-rainbow",
        "davidanson.vscode-markdownlint"
      ]
    }
  }
}
